$sql("SELECT DISTINCT `date`, `hr`, `min`, unix_timestamp(concat(`date`, ' ',  substr(cast((`hr`+100) as STRING), 2), ':', substr(cast((`min`+100) as STRING), 2), ':00')) AS unix_time, `meter_id` FROM powerminute WHERE `meter_id` = 'LIB-4' ORDER BY  unix_timestamp(concat(`date`, ' ',  substr(cast((`hr`+100) as STRING), 2), ':', substr(cast((`min`+100) as STRING), 2), ':00'))").show(10)
